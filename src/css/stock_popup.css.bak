	border: 1px solid #6B6B6B;
}

.div-header-reg {
	height: 10%;
	align-content: center;
}
.div-body-reg {
	height: 80%;
}
.div-footer-reg {
	height: 10%;
	text-align: center;
	vertical-align: middle;
	margin: 10px 0px 0px 0px;
}

.no-line{
	text-decoration:none;
}

#div-container {
	width: 550px;
	height: 350px;
	background-color: beige;
	padding: 0px;
	margin: 0px;                
}

#div-header {
	height: 10%;
	padding: 0px;
	margin: 0px;
	/*background-color: bisque;*/
}

#div-body {
	height: 80%;
	padding: 0px;
	margin: 0px;
}

#div-footer {
	height: 10%;
	/*background-color: aqua;*/
	text-align: right;
	padding: 0px;
	margin: 0px;
}

#div-footer button {
	width: 80px;
	height: 56px;
	font-weight: bold;
	font-size: 20px;
	padding: 0px;
	margin: 10px 10px 0px 0px;
	text-align: center;
}


.main-body{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
 
}
.main-body .main-top{
  position: absolute;
  top: 10px;
  width: 100%;
  height: 40px;
 
}
.main-body .main-middle1{
  position: absolute;
  top: 70px;
  width: 100%;
  height: 70px;
 
}
.main-body .main-middle2{
  position: absolute;
  top: 140px;
  width: 100%;
  height: 30px;
 
}
.main-body .body-diff{
  position: absolute;
  top: 170px;
  width: 91%;
  height: 80%;
  left:5%;
}
.main-body .main-content{
  position: absolute;
  width: 100%;
  height: 90%;
  overflow-y:scroll;
}
.main-body .main-content1{
  position: absolute;
  top:90%;
  width: 100%;
  height: 10%;
}
/*재고실사*/
.pop-stock {
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 90%;
  height: 90%;
  padding: 20px 36px;
  background-color: #fff;
  border: 5px solid #B9F6CA;
  z-index: 10;
  font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
}

.pop-stock .div-header{
	height: 5%;
	font-weight: bold;
	text-align: center;
	font-size: 11pt;
	padding: 0px;
	margin: 0px;
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	/*background-color: bisque;*/
}

.pop-stock .div-body{
	width: 100%;
	height: 90%;
	padding: 0px;
	margin: 0px;	
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	/*overflow-y:scroll;*/
}
.pop-stock .div-body .sales13font{
    width: 30%;
	font-size:7pt; 
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	text-align: right;
	color:#345487;
    float: left;
}
.pop-stock .div-body .valuefont{	
    width: 70%;
	font-size:8pt; 
	font-weight: bold;
	font-family: Malgun Gothic , Arial, Helvetica, sans-serif;
	text-align: right;
	color:#474747;
    float: right;
}



.pop-stock .div-footer{
	height: 5%;
	-background-color: aqua;
	text-align: right;
	padding: 0px;
	margin: 0px;
}

.pop-stock .pop-container {
	width: 100%;
	height: 100%;	
}

.pop-stock p.ctxt {
  color: #666;
  line-height: 36px;
}

.pop-stock .btn-r {
  width: 100%;
  margin: 10px 0 20px;
  padding-top: 10px;
  border-top: 1px solid #DDD;
  text-align: right;
}

.dim-stock {
  display: none;
  position: fixed;
  _position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.dim-stock .dimBg-stock {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: .5;
  filter: alpha(opacity=50);
}

.dim-stock .pop-stock {
  display: block;
}


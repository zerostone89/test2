/*div*/
#demoparagraph {
width:700px;
-webkit-column-count: 2;
-moz-column-count: 2;
column-count: 2;
-webkit-column-gap: 7px;
-moz-column-gap: 7px;
column-gap: 7px;
-webkit-column-rule: 2px solid #A4A4A4;
-moz-column-rule: 2px solid #A4A4A4;
column-rule: 2px solid #A4A4A4;
margin:auto;
text-align:center;
}

/* DivTable.com */
.divTable{
	display: table;
	width: 100%;
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	font-weight: bold;
	font-size: 9pt;
	letter-spacing: 2px;
	word-spacing: 2px;
	color: #6B6B6B;
}
.divTableRow {
	display: table-row;	
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
}

.divTableRow input{
	text-align:right;
	width:100%;
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
}
.divTableHeading {
	background-color: #EEE;
	display: table-header-group;
}
.divTableCell, .divTableHead {
	display: table-cell;
	padding: 3px 3px;
	text-align:right;
}
.divTableCell_center {
	width: 110px;
	height:25px;
	display: table-cell;
	padding: 3px 3px;
	text-align:right;
}
.divTableHeading {
	background-color: #EEE;
	display: table-header-group;
	font-weight: bold;
}
.divTableFoot {
	background-color: #EEE;
	display: table-footer-group;
	font-weight: bold;
}
.divTableBody {
	display: table-row-group;
}

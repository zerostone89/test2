/*반품*/
.pop-return {
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 90%;
  height: 90%;
  padding: 20px 36px;
  background-color: #fff;
  border: 5px solid #B9F6CA;
  z-index: 10;
  font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
}

.pop-return .div-header{
	height: 5%;
	font-weight: bold;
	text-align: center;
	font-size: 11pt;
	padding: 0px;
	margin: 0px;
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	/*background-color: bisque;*/
}

.pop-return .div-body{
	width: 100%;
	height: 90%;
	padding: 0px;
	margin: 0px;	
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	/*overflow-y:scroll;*/
}
.pop-return .div-body .sales13font{
    width: 30%;
	font-size:7pt; 
	font-family: Malgun Gothic ,Arial, Helvetica, sans-serif;
	text-align: right;
	color:#345487;
    float: left;
}
.pop-return .div-body .valuefont{	
    width: 70%;
	font-size:8pt; 
	font-weight: bold;
	font-family: Malgun Gothic , Arial, Helvetica, sans-serif;
	text-align: right;
	color:#474747;
    float: right;
}



.pop-return .div-footer{
	height: 5%;
	-background-color: aqua;
	text-align: right;
	padding: 0px;
	margin: 0px;
}

.pop-return .pop-container {
	width: 100%;
	height: 100%;	
}

.pop-return p.ctxt {
  color: #666;
  line-height: 36px;
}

.pop-return .btn-r {
  width: 100%;
  margin: 10px 0 20px;
  padding-top: 10px;
  border-top: 1px solid #DDD;
  text-align: right;
}

.dim-return {
  display: none;
  position: fixed;
  _position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.dim-return .dimBg-return {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: .5;
  filter: alpha(opacity=50);
}

.dim-return .pop-return {
  display: block;
}


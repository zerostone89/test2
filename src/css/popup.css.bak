.div-header-reg {
	height: 10%;
	align-content: center;
}
.div-body-reg {
	height: 80%;
}
.div-footer-reg {
	height: 10%;
	text-align: center;
	vertical-align: middle;
	margin: 10px 0px 0px 0px;
}

#div-container {
	width: 550px;
	height: 350px;
	background-color: beige;
	padding: 0px;
	margin: 0px;                
}

#div-header {
	height: 10%;
	padding: 0px;
	margin: 0px;
	-background-color: bisque;
}

#div-body {
	height: 80%;
	padding: 0px;
	margin: 0px;
}

#div-footer {
	height: 10%;
	-background-color: aqua;
	text-align: right;
	padding: 0px;
	margin: 0px;
}

#div-footer button {
	width: 80px;
	height: 56px;
	font-weight: bold;
	font-size: 20px;
	padding: 0px;
	margin: 10px 10px 0px 0px;
	text-align: center;
}

.pop-layer99 {
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 350px;
  height: 150px;
  background-color: #fff;
  border: 5px solid #B9F6CA;
  z-index: 10;
}

.pop-layer99 .pop-container{
	height: 80%;
	padding: 20px 25px;
}

.pop-layer99 .div-header{
	height: 25%;
	font-weight: bold;
	font-size: 13pt;
	padding: 0px;
	margin: 0px;
	-background-color: bisque;
}

.pop-layer99 .div-body{
	height: 50%;
	padding: 0px;
	margin: 0px;
}

.pop-layer99 .div-footer{
	height: 25%;
	-background-color: aqua;
	text-align: right;
	padding: 0px;
	margin: 0px;
}

.pop-layer99 p.ctxt {
  color: #666;
  line-height: 25px;
}

.pop-layer99 .btn-r {
  width: 100%;
  margin: 10px 0 20px;
  padding-top: 10px;
  border-top: 1px solid #DDD;
  text-align: right;
}

.dim-layer99 {
  display: none;
  position: fixed;
  _position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}

.dim-layer99 .dimBg99 {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: .5;
  filter: alpha(opacity=50);
}

.dim-layer99 .pop-layer99 {
  display: block;
}

